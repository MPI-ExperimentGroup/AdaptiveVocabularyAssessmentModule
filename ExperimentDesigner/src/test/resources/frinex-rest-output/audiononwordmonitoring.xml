<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<experiment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="/Users/olhshk/Documents/ExperimentTemplate/ExperimentDesigner/target/test-classes/frinex-rest-output/frinex.xsd" appNameDisplay="Audio non-word monitoring" appNameInternal="audiononwordmonitoring" backgroundColour="#FFFFFF" complementColour0="#EAC3A3" complementColour1="#9D7B5E" complementColour2="#C69E7C" complementColour3="#FFEDDE" complementColour4="#FFFDFB" defaultScale="1.0" isScalable="true" preserveLastState="true" primaryColour0="#628D8D" primaryColour1="#385E5E" primaryColour2="#4A7777" primaryColour3="#96ADAD" primaryColour4="#D5D8D8" rotatable="true" showMenuBar="false" textFontSize="17">
    <preventWindowClose featureText="Bij reload worden ingetypte data verwiderd"/>
    <administration>
        <dataChannel channel="4" label="Corsi" logToSdCard="false"/>
    </administration>
    <!--<experiment appNameDisplay="Adaptive Vocabulary Assessment" appNameInternal="advocas" backgroundColour="#FFFFFF" complementColour0="#EAC3A3" complementColour1="#9D7B5E" complementColour2="#C69E7C" complementColour3="#FFEDDE" complementColour4="#FFFDFB" defaultScale="1.0" isScalable="true" primaryColour0="#628D8D" primaryColour1="#385E5E" primaryColour2="#4A7777" primaryColour3="#96ADAD" primaryColour4="#D5D8D8" showMenuBar="false" textFontSize="17">-->
    <metadata>
        <field controlledMessage="Input at least 3 letters." controlledRegex=".'{'3,'}'" postName="workerId" preventServerDuplicates="false" registrationField="Wat is je proefpersoonnummer?"/>
    </metadata>
    <presenter displayOrder="1" type="menu" self="Menu" title="Menu">
        <centrePage/>
        <addPadding/>
        <htmlText featureText="&lt;div  &gt; &lt;img src=&quot;static/logo-max.png&quot; align=&quot;left&quot; /&gt; &lt;img src=&quot;static/logo-lil.png&quot; align=&quot;right&quot;/&gt; &lt;/div&gt;" styleName="fullScreenWidth"/>
        <addPadding/>
        <addPadding/>
        <addPadding/>
        <addPadding/>
        <htmlText featureText="Welkom bij Language in Interaction BQ4 experimenten."/>
        <addPadding/>
        <menuItem featureText="Audio non-word monitoring " target="Edit_User"></menuItem>
    </presenter>
    <presenter menuLabel="" displayOrder="40" next="Accord" type="metadata" self="Edit_User" title="">
        <metadataField fieldName="workerId"/>
        <saveMetadataButton featureText="Volgende" sendData="true" networkErrorMessage="Cannot connect to the server, please check your internet connection.">
            <onError/>
            <onSuccess>
                <gotoNextPresenter/>
            </onSuccess>
        </saveMetadataButton>
    </presenter>
    <presenter displayOrder="10" next="stimuliScreen" type="metadata" self="Accord" title="">
        <centrePage/>
        <htmlText featureText="&lt;center&gt; &lt;b&gt;&lt;big&gt;Instructies &lt;/big&gt; &lt;/b&gt; &lt;/center&gt;"/>
        <addPadding/>
        <addPadding/>
        <addPadding/>
        <htmlText featureText="Welkom bij dit experiment,"/>
        <addPadding/>
        <htmlText featureText="In dit audio-experiment krijg je steeds een niet-bestaand voorbeeldwoord te horen. Na dit voorbeeldwoord volgt een lijst met niet-bestaande woorden in ruis. Druk zo snel mogelijk op de spatiebalk, zodra je het voorbeeldwoord terughoort in de lijst."/>
        <addPadding/>
        <htmlText featureText="Let op: luister goed naar het voorbeeldwoord en de woorden in de lijst. In sommige lijsten komt het voorbeeldwoord niet terug. Je hoeft dan niet op de spatiebalk te drukken."/>
        <addPadding/>
        <htmlText featureText="Om het voorbeeldwoord te horen, druk je op spatie. De lijst met woorden hoor je daarna. Om het voorbeeldwoord van de volgende lijst te horen, druk je weer op spatie. "/>
        <addPadding/>
        <htmlText featureText="&lt;center&gt; &lt;b&gt;&lt;big&gt;Veel succes!&lt;/big&gt;&lt;/b&gt; &lt;/center&gt;"/>
        <addPadding/>
        <addPadding/>
        <targetButton featureText="Aan de slag" target="stimuliScreen1"/>
    </presenter>
    
    
    <presenter displayOrder="20" next="Administration" type="stimulus" self="stimuliScreen1" title="">
        <loadStimulus eventTag="stimuliN" randomise="false" repeatCount="1" maxStimuli="10">
            <hasMoreStimulus>
                <triggerListener listenerId="nextStimulusAction" maximum="1" threshold="1">
                    <nextStimulus repeatIncorrect="false"/>
                </triggerListener>
                <stimulusHasRatingOptions> <!-- not a cue, recation expected -->
                    <conditionTrue>
                        <clearPage/>
                        <centrePage/>
                        <addPadding/>
                        <addPadding/>
                        <addPadding/>
                        <addPadding/>
                        <htmlText featureText="&lt;div &gt; &lt;img src=&quot;static/star-icon-grey.png&quot; /&gt;  &lt;/div&gt;"/>
                        <stimulusButton featureText="&#160;" hotKey="SPACE" styleName="hiddenBottomRight">
                            <cancelPauseTimers/>  
                            <trigger listenerId="nextStimulusAction"/>
                        </stimulusButton>
                        <disableStimulusButtons/>
                        <stimulusAudio msToNext="0" showPlaybackIndicator="false">
                            <mediaLoadFailed>
                                <htmlText featureText="Failed to load audio file"/>
                            </mediaLoadFailed>
                            <mediaLoaded/>
                            <mediaPlaybackComplete>
                                <enableStimulusButtons/>
                                <cancelPauseTimers/> 
                                <pause  msToNext="500">
                                    <trigger listenerId="nextStimulusAction"/>
                                </pause>
                            </mediaPlaybackComplete>
                        </stimulusAudio>
                    </conditionTrue>
                    <conditionFalse> <!-- cue -->
                        <clearPage/>
                        <centrePage/>
                        <addPadding/>
                        <addPadding/>
                        <addPadding/>
                        <addPadding/>
                        <table>
                            <row>
                                <column>
                                    <htmlText featureText="&lt;div &gt;  De nieuwe serie begint. Toets SPATIE om het voorbeeldwoord te horen &lt;/div&gt;"/>
                                </column>
                            </row>
                        </table>
                        <actionButton featureText="&#160;" hotKey="SPACE" styleName="hiddenBottomRight">
                            <clearPage/>
                            <centrePage/>
                            <addPadding/>
                            <addPadding/>
                            <addPadding/>
                            <addPadding/>
                            <htmlText featureText="&lt;div &gt; &lt;img src=&quot;static/star-icon-grey.png&quot; /&gt;  &lt;/div&gt;"/>
                            <stimulusAudio msToNext="0" showPlaybackIndicator="false">
                                <mediaLoadFailed>
                                    <htmlText featureText="Failed to load audio file"/>
                                </mediaLoadFailed>
                                <mediaLoaded/>
                                <mediaPlaybackComplete>
                                    <cancelPauseTimers/> 
                                    <pause  msToNext="1000">
                                        <trigger listenerId="nextStimulusAction"/>
                                    </pause> 
                                </mediaPlaybackComplete>
                            </stimulusAudio>
                        </actionButton>
                    </conditionFalse> 
                </stimulusHasRatingOptions>
            </hasMoreStimulus>
            <endOfStimulus>
                <clearPage/>
                <centrePage/>
                <addPadding/>
                <addPadding/>
                <addPadding/>
                <addPadding/>
                <targetButton featureText="Heel goed gedaan. Nu komt de tweede ronde. Klik op de button om door te gaan. Veel Succes!" target="Administration"/>
                <addPadding/>
            </endOfStimulus>
            <randomGrouping/>
            <stimuli>
                <tag>round1</tag>
            </stimuli>          
        </loadStimulus>
    </presenter>
    
    
     
  
    <presenter displayOrder="100" type="transmission" self="Admin" title="Administratie" next="Menu">
        <sendMetadata>
            <onSuccess> 
                <sendAllData>
                    <onSuccess>
                        <addPadding/>
                        <addPadding/>
                        <addPadding/>
                        <addPadding/>
                        <htmlText featureText="Dat was het! Bedankt voor het meedoen."/>
                        <addPadding/>
                        <addPadding/>
                        <eraseUsersDataButton featureText="Locaal geheugen schoonmaken  en opniew beginnen" target="Menu"/>
                        <addPadding/>
                        <addPadding/>
                        <createUserButton featureText="Opnieuw beginnen" target="Menu"/>
                    </onSuccess>
                    <onError>
                        <plainText featureText="Cannot connect to the server, please check your internet connection and/or upload data later. Do not clean the browser's cache."/>
                        <addPadding/>
                        <addPadding/>
                        <targetButton featureText="Retry" target="Admin"/>
                        <addPadding/>
                        <addPadding/>
                        <createUserButton featureText="Opnieuw beginnen" target="Menu"/>
                    </onError>
                </sendAllData>
            </onSuccess>
            <onError>   
                <plainText featureText="Cannot connect to the server, please check your internet connection and/or upload data later. Do not clean the browser's cache."/>
                <addPadding/>
                <addPadding/>
                <targetButton featureText="Retry" target="Admin"/>
                <addPadding/>
                <addPadding/>
                <createUserButton featureText="Opnieuw beginnen" target="Menu"/>
            </onError>      
        </sendMetadata>
    </presenter>
    <presenter back="Menu" displayOrder="110"  menuLabel="about" type="debug" self="about" title="about">
        <versionData/>
        <eraseLocalStorageButton/>
        <localStorageData/>
    </presenter>
    <stimuli> 
    </stimuli>

</experiment>
